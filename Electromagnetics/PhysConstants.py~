## PhysConstants.py
## some physical constants

from math import pi

__all__ = ["SI"]


class SI:
    "SI constants"    
    e = 1.60217657e-19         # C
    eps_0 = 8.854187817e-12    # F/m
    mu_0 = 4.0*pi*1.0-7        # H/m
    c = 299792458              # m/s
    k_B = 1.3806488e-23        # m2 kg s-2 K-1
    h = 6.62606957e-34         # m2 kg / s
    h_bar = h/(2.0*pi)
    




        
